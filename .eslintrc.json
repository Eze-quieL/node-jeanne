{
	"env": {
		"browser": true,
		"commonjs": true,
		"es6": true,
		"node": true
	},
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"sourceType": "module"
	},
	"rules": {
		"no-const-assign": "error",
		"no-this-before-super": "error",
		"no-undef": "error",
		"no-unreachable": "error",
		"no-unused-vars": "off",
		"constructor-super": "warn",
		"valid-typeof": "warn",
		"async-await/space-after-async": 2,
		"async-await/space-after-await": 2
	},
	"extends": "eslint:recommended",
	"plugins": [
		"async-await"
	]
}